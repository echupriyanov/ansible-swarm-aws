"version": 1
"control-service":
   "hostname": "{{groups[flocker_master_group][0]}}"
   "port": 4524

# The dataset key below selects and configures a dataset backend (see below: aws/openstack/etc).
# All nodes will be configured to use only one backend

dataset:
   backend: "aws"
   region: "eu-west-1"
   zone: "{{zone}}"
   access_key_id: "{{aws_accesskey}}"
   secret_access_key: "{{aws_secretkey}}"
